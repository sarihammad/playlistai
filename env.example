# Data processing
SYMBOLS=TRACKS_ONLY
SESSION_GAP_MIN=45
MAX_SEQ_LEN=50
VOCAB_MIN_FREQ=5

# API configuration
API_PORT=8088
REDIS_URL=redis://redis:6379/0
USE_FAISS=false
PROM_PATH=/metrics

# Model configuration
MODEL_NAME=transformer
MODEL_D=256
LOSS_LABEL_SMOOTH=0.1

# Performance tuning
COST_BPS=3
CAND_TOPK=500

# Training
BATCH_SIZE=128
LEARNING_RATE=3e-4
NUM_EPOCHS=10
DROPOUT=0.1
WEIGHT_DECAY=1e-4

# Ray configuration
RAY_NUM_WORKERS=2
RAY_USE_GPU=false

